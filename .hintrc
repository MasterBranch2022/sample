    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/aria": [
      "default",
      {
        "aria-progressbar-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select"
        ]
      }
